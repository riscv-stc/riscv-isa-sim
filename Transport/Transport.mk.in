Transport_CFLAGS = -fPIC

Transport_srcs = \
  proxy.grpc.pb.cc \
  proxy.pb.cc \
  Interface.cc \
  Framework.cc \
  FrameworkGrpc.cc \
  Stream.cc \
  StreamMessage.cc \

Transport_gen_hdrs = \
proxy.grpc.pb.h \
proxy.pb.h \

proxy.pb.cc : proxy.proto
	protoc --cpp_out=.  --proto_path=$(src_dir)/Transport proxy.proto

proxy.pb.h : proxy.proto
	protoc --cpp_out=.  --proto_path=$(src_dir)/Transport proxy.proto

proxy.grpc.pb.cc :proxy.pb.cc
	protoc --grpc_out=. --plugin=protoc-gen-grpc=`which grpc_cpp_plugin` --proto_path=$(src_dir)/Transport proxy.proto

proxy.grpc.pb.h :proxy.pb.cc
	protoc --grpc_out=. --plugin=protoc-gen-grpc=`which grpc_cpp_plugin` --proto_path=$(src_dir)/Transport proxy.proto
